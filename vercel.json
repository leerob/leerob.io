{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/ibm-plex-sans-var-italic.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/resume",
      "destination": "https://standardresume.co/r/satvik"
    },
    {
      "source": "/projects",
      "destination": "/about"
    },
    {
      "source": "/youtube",
      "destination": "https://www.youtube.com/channel/UC7C8Kc8fU4s0UpWQqlJ4ocA"
    }
  ]
}
